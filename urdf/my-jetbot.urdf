<?xml version="1.0"?>
<robot name="jetbot">

<!-- odom, so base_link can have inertia -->
<link name="odom"/>

<!-- robot base -->
  <link name="base_link">
    <visual>
      <geometry>
        <box size="0.13954 0.0995 0.003"/>
      </geometry>
    </visual>
    <collision>
      <geometry>
        <box size="0.13954 0.0995 0.003"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="0.5"/>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <inertia ixx="0.0008333333333333335" ixy="0" ixz="0" iyy="0.0008333333333333335" iyz="0" izz="0.0008333333333333335"/>
    </inertial>
  </link>

  <joint name="base_link_base" type="fixed">
    <origin rpy="0 0 0" xyz="0 0 0.0445" />
    <parent link="odom" />
    <child link="base_link" />
  </joint>

  <gazebo reference="base_link">
    <material>Gazebo/Black</material>
  </gazebo>

<!-- standoff 1 -->
   <link name="standoff1">
    <visual>
      <origin rpy="0 0 0" xyz="0 0 0" />
      <geometry>
        <cylinder radius="0.0025" length="0.0393" />
      </geometry>
    </visual>
    <collision>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <cylinder radius="0.0025" length="0.0393" />
      </geometry>
    </collision>
    <inertial>
      <mass value="0.5"/>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <inertia ixx="2e-05" ixy="0.0" ixz="0.0" iyy="2e-05" iyz="0.0" izz="2e-05"/>
    </inertial>
  </link>

  <joint name="standoff_joint1" type="fixed">
    <origin rpy="0 0 0" xyz="-0.06277 0.042975 0.01975" />
    <parent link="base_link" />
    <child link="standoff1" />
  </joint>

  <gazebo reference="standoff1">
    <material>Gazebo/Gold</material>
  </gazebo>

